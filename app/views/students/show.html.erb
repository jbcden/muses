<h1><%= current_student.email %></h1>

<div><%= link_to 'Edit Profile', edit_student_registration_path %></div>

<div><a href='<%= new_campaign_path %>'>Create a new campaign here</a></div>

<div>
  <p>Campaigns</p>
  <ul>
    <% for c in current_student.campaigns %>
      <!-- <li><%= c.profile_picture %></li> -->
      <a href='<%= campaign_path c %>'><%= image_tag c.profile_picture.url :thumb %></a>
    <% end%>
  </ul>
</div>

